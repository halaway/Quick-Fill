{
  "name": "Quick Fill",
  "version": "1.0.0",
  "description": "A quick and easy way to fill out an account sign-on form!",
  "manifest_version": 3,
  "author": "Daniel Alfaro",
  "action": {
    "default_popup": "index.html",
    "default_title": "Quick Fill"
  },
  "permissions": [
    "storage",
    "activeTab",
    "declarativeContent"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [{
    "matches": [
      "https://*.myworkdayjobs.com/*",
      "https://*.workday.com/*"
    ],
    "js": ["document.js"]
  }]
}